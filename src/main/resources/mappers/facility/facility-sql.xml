<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="facility">
	<select id="select" resultType="Facility">
		SELECT *
		FROM EO_FACILITY
		ORDER BY FACILITY_TAG_NO
	</select>
