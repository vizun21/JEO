	<entry key="database.driverClassName">org.mariadb.jdbc.Driver</entry>
	<entry key="database.url">jdbc:mariadb://</entry>
	<entry key="database.username"></entry>
	<entry key="database.password"></entry>

	<!-- BankdaAccount -->
	<entry key="bankdaAccount.SERVICE_TYPE"></entry>
	<entry key="bankdaAccount.PARTNER_ID"></entry>
